{
  "name": "va/wp-plugin-version-list",
  "description": "Wordpress Plugin to send current Version Information to API",
  "type": "wordpress-plugin",
  "keywords": [
    "php",
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "VersionList\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.4 | ^7.4"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
